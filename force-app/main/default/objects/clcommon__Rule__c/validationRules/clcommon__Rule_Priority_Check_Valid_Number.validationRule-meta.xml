<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>clcommon__Rule_Priority_Check_Valid_Number</fullName>
    <active>true</active>
    <description>Check if rule priority less than or equal to 0</description>
    <errorConditionFormula>NOT(ISNULL(clcommon__Rule_Priority__c))&amp;&amp; clcommon__Rule_Priority__c &lt;= 0</errorConditionFormula>
    <errorMessage>Rule Priority should be greater than 0.</errorMessage>
</ValidationRule>
