<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>talkdesk__Month_of_Year__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>case(IF(ISBLANK(talkdesk__Created_Time__c),
            value(mid(text(talkdesk__Start_Time__c + (talkdesk__user_timezone__c / 24)), 6, 2)), value(mid(talkdesk__Created_Time__c, 6, 2))),
            1, &quot;January&quot;,
            2, &quot;February&quot;,
            3, &quot;March&quot;,
            4, &quot;April&quot;,
            5, &quot;May&quot;,
            6, &quot;June&quot;,
            7, &quot;July&quot;,
            8, &quot;August&quot;,
            9, &quot;September&quot;,
            10, &quot;October&quot;,
            11, &quot;November&quot;,
            &quot;December&quot;
            )</formula>
    <label>Month of Year</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
