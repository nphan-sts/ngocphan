<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>genesis__Social_Security_Number_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(OR(LEN (genesis__Social_Security_Number__c) = 0,REGEX( genesis__Social_Security_Number__c , &quot;[0-9]{3}-[0-9]{2}-[0-9]{4}&quot;)))</errorConditionFormula>
    <errorDisplayField>genesis__Social_Security_Number__c</errorDisplayField>
    <errorMessage>SSN must be in this format: 999-99-9999.</errorMessage>
</ValidationRule>
