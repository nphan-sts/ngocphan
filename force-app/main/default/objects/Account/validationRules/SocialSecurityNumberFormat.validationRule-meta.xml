<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SocialSecurityNumberFormat</fullName>
    <active>true</active>
    <description>loan__SOCIAL_SECURITY_NUMBER__c must be nine digit numeric</description>
    <errorConditionFormula>IF((ISNULL(loan__SOCIAL_SECURITY_NUMBER__c) || loan__SOCIAL_SECURITY_NUMBER__c = &apos;&apos;), FALSE, (loan__Investor__c = FALSE &amp;&amp; (ISNUMBER(loan__SOCIAL_SECURITY_NUMBER__c)=FALSE || 
LEN(loan__SOCIAL_SECURITY_NUMBER__c) &lt;&gt; 9)))</errorConditionFormula>
    <errorMessage>Social Security Number must be nine digit, numeric value.</errorMessage>
</ValidationRule>
