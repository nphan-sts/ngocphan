<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>loan__ExternalID_Special_char_check</fullName>
    <active>true</active>
    <description>ExternalID_Special_char_check</description>
