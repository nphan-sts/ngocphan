<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>genesis__Upvote_Ratio__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>IF(
     (genesis__Total_Upvotes__c + genesis__Total_Downvotes__c) &gt; 0,
     genesis__Total_Upvotes__c / (genesis__Total_Upvotes__c + genesis__Total_Downvotes__c),
     0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Upvote Ratio</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
