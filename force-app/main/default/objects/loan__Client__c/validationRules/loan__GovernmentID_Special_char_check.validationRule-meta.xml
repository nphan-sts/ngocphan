<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>loan__GovernmentID_Special_char_check</fullName>
    <active>true</active>
    <description>GovernmentID_Special_char_check</description>
    <errorConditionFormula>NOT(REGEX(  loan__Government_ID__c  , &quot;^[a-zA-Z0-9-\\s]*$&quot;))</errorConditionFormula>
    <errorDisplayField>loan__Government_ID__c</errorDisplayField>
    <errorMessage>GovernmentID should not include special characters</errorMessage>
</ValidationRule>
