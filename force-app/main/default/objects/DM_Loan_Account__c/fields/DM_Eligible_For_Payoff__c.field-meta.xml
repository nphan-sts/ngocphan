<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DM_Eligible_For_Payoff__c</fullName>
    <description>Check if the loan contract is eligible for loan payoff.</description>
    <externalId>false</externalId>
    <formula>DM_Fees_Remaining__c +  DM_Interest_Remaining__c +  DM_Principal_Remaining__c &lt;=  DM_Excess__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Check if the loan contract is eligible for loan payoff.</inlineHelpText>
    <label>DM Eligible For Payoff</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
