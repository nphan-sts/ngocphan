<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>clcommon__Alias_Alphanumeric</fullName>
    <active>true</active>
    <description>Alias must be alphanumeric and without spaces</description>
    <errorConditionFormula>AND(
    NOT(ISBLANK(clcommon__Alias__c)),
    NOT(REGEX(clcommon__Alias__c,&apos;[a-zA-Z0-9]+&apos;))
)</errorConditionFormula>
    <errorMessage>Alias must be alphanumeric and without spaces</errorMessage>
</ValidationRule>
