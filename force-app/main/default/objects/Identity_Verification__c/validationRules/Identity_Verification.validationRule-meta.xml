<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Identity_Verification</fullName>
    <active>false</active>
    <errorConditionFormula>AND(   
   ISCHANGED(Identity_Verification__c ),
   ISPICKVAL(Identity_Verification__c, &quot;PASS&quot;),  
   count__c &lt;= 3
)</errorConditionFormula>
    <errorMessage>Identity Verification Cannot be Done.</errorMessage>
</ValidationRule>
