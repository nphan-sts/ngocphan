<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Number_and_Funding_Account</fullName>
    <active>false</active>
    <description>Account Number and Funding Account Should be same</description>
    <errorConditionFormula>AND( Account_Number__c != NULL, Account_Number__c 
 != loan__Bank_Account_Number__c)</errorConditionFormula>
    <errorDisplayField>Account_Number__c</errorDisplayField>
    <errorMessage>Account Number and Funding Account Should be same</errorMessage>
</ValidationRule>
