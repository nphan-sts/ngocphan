<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>collect__Aging__c</fullName>
    <deprecated>false</deprecated>
    <description>The time-period range for which the contract has been overdue</description>
    <externalId>false</externalId>
    <formula>IF(AND(collect__Days_Past_Due__c&gt;0,collect__Days_Past_Due__c&lt;=30), "0-30", IF(AND(collect__Days_Past_Due__c&gt;30,collect__Days_Past_Due__c&lt;=60),"31-60",
IF(AND(collect__Days_Past_Due__c&gt;60,collect__Days_Past_Due__c&lt;=90),"61-90",
IF(AND(collect__Days_Past_Due__c&gt;90,collect__Days_Past_Due__c&lt;=120),"91-120",
IF(AND(collect__Days_Past_Due__c&gt;120,collect__Days_Past_Due__c&lt;=180),"121-180",
IF(collect__Days_Past_Due__c&gt;180,"180+",
""))))))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Aging</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
