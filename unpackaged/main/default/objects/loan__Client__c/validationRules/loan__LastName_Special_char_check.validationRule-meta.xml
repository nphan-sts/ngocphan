<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>loan__LastName_Special_char_check</fullName>
    <active>true</active>
    <description>LastName_Special_char_check</description>
    <errorConditionFormula>NOT(REGEX(   loan__Last_Name__c   , "^[a-zA-Z0-9-\\s]*$"))</errorConditionFormula>
    <errorDisplayField>loan__Last_Name__c</errorDisplayField>
    <errorMessage>Last Name should not include special characters</errorMessage>
</ValidationRule>
