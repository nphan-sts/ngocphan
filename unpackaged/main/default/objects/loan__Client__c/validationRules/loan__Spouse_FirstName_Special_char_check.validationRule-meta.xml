<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>loan__Spouse_FirstName_Special_char_check</fullName>
    <active>true</active>
    <description>Spouse_FirstName_Special_char_check</description>
    <errorConditionFormula>NOT(REGEX(     loan__Spouse_Father_s_First_Name__c    , "^[a-zA-Z]*$"))</errorConditionFormula>
    <errorDisplayField>loan__Spouse_Father_s_First_Name__c</errorDisplayField>
