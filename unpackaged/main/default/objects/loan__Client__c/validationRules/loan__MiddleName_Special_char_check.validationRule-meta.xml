<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>loan__MiddleName_Special_char_check</fullName>
    <active>true</active>
    <description>MiddleName_Special_char_check</description>
    <errorConditionFormula>NOT(REGEX(    loan__Middle_Name__c   , "^[a-zA-Z]*$"))</errorConditionFormula>
    <errorDisplayField>loan__Middle_Name__c</errorDisplayField>
    <errorMessage>Middle Name should not include special characters</errorMessage>
</ValidationRule>
