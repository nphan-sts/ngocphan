<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>clcommon__ACH_Start_Date_greater_than_End_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(clcommon__Type__c,'RECURRING') , clcommon__Recurring_ACH_End_Date__c  &lt; clcommon__Recurring_ACH_Start_Date__c )</errorConditionFormula>
    <errorMessage>ACH End Date should be greater than ACH Start Date</errorMessage>
</ValidationRule>
