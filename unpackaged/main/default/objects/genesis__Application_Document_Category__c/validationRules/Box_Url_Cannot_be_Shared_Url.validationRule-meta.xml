<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Box_Url_Cannot_be_Shared_Url</fullName>
    <active>true</active>
    <errorConditionFormula>IF( CONTAINS( Box_URl__c , '/s/'),true,false)</errorConditionFormula>
    <errorDisplayField>Box_URl__c</errorDisplayField>
    <errorMessage>Please enter a valid, NON-shared box URL</errorMessage>
</ValidationRule>
