<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>loan__Manual_Payment_Fees_Check</fullName>
    <active>false</active>
    <description>This is deprecated and not maintained.</description>
    <errorConditionFormula>ISNEW() &amp;&amp; loan__Manual_Payment__c &amp;&amp;  (ROUND(loan__Fees__c,2) &gt;   ROUND(loan__Loan_Account__r.loan__Fees_Remaining__c,2)  )</errorConditionFormula>
