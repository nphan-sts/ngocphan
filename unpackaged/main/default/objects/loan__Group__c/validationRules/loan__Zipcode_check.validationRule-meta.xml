<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>loan__Zipcode_check</fullName>
    <active>true</active>
    <description>Zipcode_check</description>
    <errorConditionFormula>NOT(REGEX(  loan__Zipcode__c , "\\d{6}"))</errorConditionFormula>
    <errorDisplayField>loan__Zipcode__c</errorDisplayField>
    <errorMessage>Invalid Zipcode</errorMessage>
</ValidationRule>
