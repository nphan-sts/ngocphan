<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>loan__Address2_Special_char_check</fullName>
    <active>true</active>
    <description>Address2_Special_char_check</description>
    <errorConditionFormula>NOT(REGEX(   loan__Address_2__c   , "^[a-zA-Z0-9-&amp;/#\\s]*$"))</errorConditionFormula>
    <errorDisplayField>loan__Address_2__c</errorDisplayField>
    <errorMessage>Address should not include special characters</errorMessage>
</ValidationRule>
