<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>peer__Contract_Date_Quarter__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>TEXT(YEAR(loan__Disbursal_Date__c)) &amp; " " &amp; If(MONTH(loan__Disbursal_Date__c) &lt;= 3, "Q1",
  If(MONTH(loan__Disbursal_Date__c) &lt;= 6, "Q2",
    If(MONTH(loan__Disbursal_Date__c) &lt;= 9, "Q3", "Q4")))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Contract Date Quarter</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
